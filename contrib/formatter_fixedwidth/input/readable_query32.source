set datestyle = 'ISO, DMY';

DROP EXTERNAL TABLE IF EXISTS tbl_ext_fixedwidth;
CREATE READABLE EXTERNAL TABLE tbl_ext_fixedwidth (
  s1 char(10))
LOCATION (
  'file://@hostname@@abs_srcdir@/data/fixedwidth_large_file.tbl'
)
FORMAT 'CUSTOM' (formatter='fixedwidth_in', s1='10');

\d tbl_ext_fixedwidth;

SELECT * FROM tbl_ext_fixedwidth ORDER BY s1 desc limit 10;
