-- Greenplum specific access method tests, in addition to what's
-- covered by upstream create_am.sql tests
\set HIDE_TABLEAM off
create table create_am_gp_ao1 (a int, b int) using ao_row distributed by (a);
\d+ create_am_gp_ao1
